		          LEFT JOIN MM_PUB_PART_SUPPLIER D    ON A.PART_NO = D.PART_NO
		          LEFT JOIN MM_PUB_SUPPLIER E       ON D.SUPPLIER_NO = E.SUPPLIER_NO
		          LEFT JOIN MM_PUB_PART G           ON A.PART_NO = G.PART_NO
		          LEFT JOIN MM_PUB_DATA_DICT Z      ON A.PROPOSAL_STATUS = Z.CODE_VALUE AND Z.CODE_TYPE = 'PKG_STATUS'

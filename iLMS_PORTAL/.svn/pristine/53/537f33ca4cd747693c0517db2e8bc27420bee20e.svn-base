	/**
	 * 数据导入初步检查的方法
	 * @param importModel
	 * @author zmj
	 * @date 2018年9月21日
	 */
	public static void checkImportRouteMessage(PupRouteMessageModel importModel) {
		StringBuffer checkInfo = new StringBuffer();
		
		if(StringUtil.isEmpty(importModel.getArea())) {
			checkInfo.append("取货区域为空;");
		}
		if(StringUtil.isEmpty(importModel.getSupplierNo())) {
			checkInfo.append("供应商代码为空;");
		}
		if(StringUtil.isEmpty(importModel.getPickupType())) {
			checkInfo.append("物流模式为空;");
		}
		if(StringUtil.isEmpty(importModel.getRecShiftA())) {
			checkInfo.append("A班收货人为空;");
		}
		if(StringUtil.isEmpty(importModel.getRecShiftB())) {
			checkInfo.append("B班收货人为空;");
		}
		if(StringUtil.isEmpty(importModel.getNwlManager())) {
			checkInfo.append("内物流管理员为空;");
		}
		if(StringUtil.isEmpty(importModel.getNwlManager())) {
			checkInfo.append("供应商出货日期为空;");
		}
		if(StringUtil.isEmpty(importModel.getNwlManager())) {
			checkInfo.append("供应商出货时间为空;");
		}
		if(StringUtil.isEmpty(importModel.getNwlManager())) {
			checkInfo.append("批次取货标识为空;");
		}
		
		if(checkInfo == null || "".equals(checkInfo.toString())){
            importModel.setCheckResult("1");
            importModel.setOpeType("I");
            importModel.setCheckInfo("");
        }else{
        	importModel.setCheckResult("0");
        	importModel.setOpeType("U");
        	importModel.setCheckInfo(checkInfo.toString());
        }
	}

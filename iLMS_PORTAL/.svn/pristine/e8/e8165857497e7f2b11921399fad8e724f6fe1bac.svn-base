/** css初始化 **/
body,html
 {
  margin: 0 0 0 0;
  padding:0 0 0 0;
  font-size: 12px;
  font-family:Microsoft YaHei,Microsoft JhengHei,STHeiti,MingLiu;
  width:100%;
  height:99.6%;
  border: none;
}
.fa{font-family:Microsoft YaHei,Microsoft JhengHei,STHeiti,MingLiu;}

/* for ie 字体的样式*/ 
button, input, select, textarea,li,a
{
  font-size: 12px;
  font-family:Microsoft YaHei,Microsoft JhengHei,STHeiti,MingLiu;
}
/*图标字体的样式*/ 
.fa-home:before{font-size:16px;}
/** 清楚列表样式 **/
ul, ol { list-style: none; }

a { text-decoration: none; font-family:Microsoft YaHei,Microsoft JhengHei,STHeiti,MingLiu; cursor:pointer;}	
a:hover { text-decoration: underline;}
/*列表的颜色定义*/
.dark {
	color: #333333 !important;
}
.white {
	color: #ffffff !important;
}

.red {
	color: #dd5a43;
}
.blue {
	color: #478fca !important;
}
.green {
	color: #69aa46 !important;
}
.orange {
	color: #ff892a !important;
}


/*侧栏nav需要设置和导航一样的颜色 */
.nav .open>a, .nav .open>a:focus, .nav .open>a:hover {
	background: #07A695;
	color: #fff;
	font-family:Microsoft YaHei,Microsoft JhengHei,STHeiti,MingLiu;
}

/* table-form css*/
table.table-list {
 	width: 99.6%;
 	font-size: 12px;
 	margin:2px auto;
 	border:1px solid #e5e5e5;
}
table.table-list thead {
}

table.table-list thead > tr > td{
	text-align: left;
	padding-left: 5px;
	
}

table.table-list  tr {
	height: 32px;
}
table.table-list thead th {
	background: #FDFDFF;
	height: 32px;
	text-align: center;
	background-color: #e5e5e5
}
table.table-list tr > th {
	background-color: #FBFCFF;
	padding: 0px 5px;
	text-align: center;
	color: #282831;
	font-weight: bold;
	font-size:12px;
	border:1px solid #e5e5e5;
}
table.table-list td {
	font-weight: normal;
	padding: 5px 5px;
	border:1px solid #e5e5e5;
}

input.ng-valid, textarea.ng-valid, select.ng-valid, .edui-editor.ng-valid, div.ng-valid .ht-input {
    border-color: #5eb95e !;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
}
input.ng-invalid, textarea.ng-invalid, select.ng-invalid, .edui-editor.ng-invalid, span.ng-invalid input, div.ng-invalid .ht-input,.has-error {
    border-color: #dd514c !important;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
}
/*edit添加页面后的背景色*/
.window {
    background: #c90021!important;
    border-radius: inherit;
}
.window div.panel-title{color:#fff;}
/** 解决easyui的样式清除边框样式 **/
.panel-body-noheader {
    border:none !important;
}
/* toolbar-panel css*/
.toolbar-panel {
	clear: both;
	padding: 2px 0 2px 2px;
	min-height: 40px;
	background-color: #F4F6FD;
	border-bottom: 1px solid silver;
}
.toolbar-panel-special{
	clear: both;
	padding: 7px 12px 7px 15px;
	min-height: 40px;
	background-color: #F4F6FD;
	border-bottom: 1px solid silver;
	position:fixed;
	width:495px;
	z-index:888;
	margin-left:-15px;
}
.toolbar-panel .buttons {
	display: inline-block;
	padding: 0 0 0 0px !important;
	margin: 3px;
}
div.panel-title{
	font-size: 14px;
    font-weight: bold;
    color: #333;
    height: 25px;
    line-height: 25px;
    margin-left: 6px;
}

.accordion .accordion-header{
	background: #0081C2;
}

.accordion .accordion-body{
	border-right: 1px solid #0081C2;
}

.panel-header{
	background: #e2e2e2;
}

/**可用不可用样式 配合ht-boolean */
.btn.active {
	color: #fff;
	background-color: #5cb85c;
	border-color: #4cae4c;
	margin-top:5px;
}

.btn.inactive {
	background-color: #cacaca;
	color: white;
	margin-top:5px;
}
/* tabs-icon的样式 */
.tabs-icon {
	margin-top:-6px !important;
	font-size:14px;
}
/* 弹出错误框的样式 */
.div_error{
	height: 98%;
}
.div_error_msg{
	color:red;
	padding:2px 5px;
	font-size:16px;
	font-weight:bold;
}
.div_error_detail{
	width:100% !important;
	height:100% !important;
}

.div_error_content{
	color:#000;
	width:100% !important;
	height:95% !important;
	padding:2px;
}
/* input 框宽度*/
.input-width-100 {
    width: 100%;
}
.input-width-80 {
    width: 80% !important;
}

.input-width-60 {
    width: 60% !important;
}

.input-width-50 {
    width: 50% !important;
}

.input-width-20 {
    width: 20% !important;
}

.input-width-10 {
    width: 10% !important;
}

.input-width-5 {
    width: 5% !important;
}

/* input 框宽度
.ng-scope{overflow-x: hidden; overflow-y: auto;}
*/

.redact-toolbar{
	padding-top: 5px; 
	padding-bottom: 5px; 
	margin-left:5px;
	float:left; 
	min-width:580px;
	position:fixed;
	background-color:#fff;
}
.react-occupied{ 
	height:43px; 
	float:left; 
	width:580px;
}

.nav-pills > li > a{ border:1px solid #cfd7dc; text-align:center;  padding:0 15px; font-size:14px; line-height:30px; margin-right:5px;}
.form-inline .control-label{ float:left; text-align:right; line-height:30px;}
.form-inline {display:block; float:left; width:100%; margin-top:5px;}
.right-operation{float:left; display:block;}
.well{float:left; width:100%; margin-top:5px;}
.operation_text{color:red; float:left; line-height:30px; margin-right:5px;}
.table-nest{width:100%; float:left; display:inlinr-block; clear:both; margin-bottom:5px;}
.table-ch{clear:both;}

.toolbar-title{font-size:16px; font-weight:bold; padding:5px;}

.ht-bpm-buttons a{margin-right:5px;}
.datagrid-header{height:40px;}
.mt55{margin-top:55px;}
select {border-radius:3px; border: 1px solid #E3E3E3; padding:3px 5px; color:#6b6b6b;}
.owner-span {
    font-size: 12px;
    background-color: #EFF2F7;
    border: 1px solid #CCD5E4;
    border-radius: 2px;
    cursor: default;
    float: left;
    height: 30px;
    margin: 0 5px 5px 5px;
    overflow: hidden;
    padding: 3px 5px;
    white-space: nowrap;
    min-height: 15px;
    position: relative;
    color: #7A7A83;
}
.owner-span a{color:#7A7A83;line-height:inherit;min-height: 12px; height:20px;}

.panel-body{
	overflow: hidden;
}
  
@media print {   
	.noprint {    
		display: none    
	}   
}
div.window-body{
	overflow-y:hidden;
}

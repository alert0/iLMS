package com.hanthink.mes.print.constants;

import java.util.ArrayList;
import java.util.List;


/**
 * print status
 * 
 * @author PanLei
 *
 */
public class PrintStatus
{

	public static final String JOB_INIT = "INIT";
	
	public static final String IN_CACHE = "IN CACHE";
	
	public static final String ESCAPE = "ESCAPE";

	public static final String PRINTED = "PRINTED";

	public static List<String> PRINT_STATUS = new ArrayList<String>();
	static
	{
		PRINT_STATUS.add(JOB_INIT);
		PRINT_STATUS.add(IN_CACHE);
		PRINT_STATUS.add(ESCAPE);
		PRINT_STATUS.add(PRINTED);
	}
	
	public static List<String> getPrintStatus()
	{
		return PRINT_STATUS;
	}
}

<style type="text/css">
html, body {
    overflow: visible;
    overflow-x: hidden;
    height: 100%;
}

.my-query {
    overflow: visible;
    line-height: 25px;
}

.my-result-ifconfig {
    height: 292px;
}

.my-result-queryconfig, 
.my-result-receiveconfig, 
.my-result-param,
.my-result-generalparam,
.my-result-element {
    height: 400px;
}

.my-location {
    position: absolute;
    z-index: 99;
    right: 20px;
    width: 40px;
    opacity: 1;
    top: 50%;
    transform: translateY(-50%);
}

.my-location i {
    margin-left: -3px;
}

.my-result-templategroup {
    width: 325px;
    height: 200px;
}

.my-result-templateparam {
    margin-left: 326px;
    height: 200px;
}

.my-result-selectparam {
    height: 200px;
}

textarea[name="content"] {
    height: 240px;
    word-wrap:normal;
}

.tab-content {
    border-left: 1px #ddd solid;
    border-bottom: 1px #ddd solid;
    border-right: 1px #ddd solid;
    padding-top: 10px;
    border-radius: 0px 0px 4px 4px;
}

.my-tab-checkbox {
    position: absolute;
    z-index: 9;
    color: rgb(128, 128, 128);
    top: 10px;
    left: 5px;
}

.my-tab-checkbox + a {
    width: 100px;
}

.my-fieldtype-container {
    height: 54px;
}
</style>

</head>
<body>

  <ul class="nav nav-primary nav-stacked my-location">
    <li><a href="#top"><i class="icon icon-arrow-up"></i></a></li>
    <li><a href="#middle"><i class="icon icon-arrow-down"></i></a></li>
  </ul>

  <!-- 查询条件 -->
  <a name="top"></a>
